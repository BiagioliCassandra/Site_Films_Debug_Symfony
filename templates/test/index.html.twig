{% extends 'layout.html.twig' %}
{% block title %}Accueil{% endblock %}
{% block pageTitle %}Accueil{% endblock %}
{% block main %}
  {% for movie in movies %}
    <div class="card">
      <img class="card-img-top" src="test" alt="test">
      <div class="card-body">
        <h5 class="card-title">{{ movie.title }}</h5>
        <p class="card-text">{{ movie.sumary }}</p>
        <a class="btn btn-primary" href="{{ path('evaluation', {'id' : movie.id}) }}">Evaluer</a>
        <a class="btn btn-primary" href="{{ path('single', {'id' : movie.id}) }}">Voir</a>
      </div>
    </div>
  {% endfor %}
{% endblock %}


